        <mesh filename="package://tracked_robot_test/meshes/09wheel.dae" scale="${scalling} ${scalling} ${scalling}"/>
      </geometry>
    </visual>
  </link>
  <joint name="left_front_bar_wheel_2_hinge" type="continuous">
    <parent link="left_front_bar"/>
    <child link="left_front_bar_wheel_2"/>
    <origin rpy="0 0 0" xyz="${(x_size/2-0.01)/4} 0 ${h_bar + height/2}"/>
    <axis xyz="0 0 1"/>
            <dynamics damping="${damping}" friction="${friction}"/>
    <limit effort="100.0" velocity="5.0"/>
  </joint>
  <gazebo reference="left_front_bar_wheel_2">
      <mu1>${mu1}</mu1> 
      <mu2>${mu2}</mu2> 
      <kp>${kp}</kp>
      <kd>${kd}</kd>
      <maxVel>${maxVel}</maxVel>
      <fdir1>${fdir1}</fdir1>

      <minDepth>${minDepth}</minDepth>
      <maxContacts>${maxContacts}</maxContacts>
  </gazebo>
  <transmission name="left_front_bar_wheel_2_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <actuator name="left_front_bar_wheel_motor_2">
