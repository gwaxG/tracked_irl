    </inertial>
    <collision name="left_front_bar_wheel_2_collision">
      <geometry>
        <mesh filename="package://tracked_robot_test/meshes/09wheel.dae" scale="${scalling} ${scalling} ${scalling}"/>
      </geometry>
    </collision>
